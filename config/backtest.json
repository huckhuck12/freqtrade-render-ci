{
  "$schema": "https://schema.freqtrade.io/schema.json",
  "dry_run": true,
  "stake_currency": "USDT",
  "stake_amount": "unlimited",
  "tradable_balance_ratio": 0.99,
  "timeframe": "5m",
  "fiat_display_currency": "USD",
  "exchange": {
    "name": "binance",
    "key": "",
    "secret": "",
    "ccxt_config": {
      "enableRateLimit": true
    }
  },
  "pairlists": [
    {
      "method": "StaticPairList",
      "pairs": [
        "SOL/USDT",
        "AVAX/USDT",
        "OP/USDT",
        "ARB/USDT"
      ]
    }
  ],
  "max_open_trades": 1,
  "starting_balance": 1000,
  "dry_run_wallet": 1000,
  "stoploss": -0.15,
  "unfilledtimeout": {
    "entry": 10,
    "exit": 10
  }
}